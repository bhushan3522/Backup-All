package SimplePrograms;


public class ArrMinMax {

	public static void main(String[] args) {
		
		int a[] = {3,2,5,9,10,1};
		
		int min = a[0];
		int max = a[0];
		
		for(int i =0 ; i< a.length;i++)
		{
			if(a[i] < min )
				min = a[i];
				
		}
		System.out.println("min number is :"+min);
		//System.out.println("max number is :"+max);
	}

}
